{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": [
      "es2018"
    ],
    "outDir": "../dist/authentication",
    "sourceMap": false,
    "strict": true,
    "strictNullChecks": false,
    "resolveJsonModule": true,
    "moduleResolution": "node",
    "typeRoots": [
      "node_modules/@types"
    ],
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  },
  "include": [
    "../src/authentication"
  ],
  "exclude": [
    "../node_modules"
  ]
}